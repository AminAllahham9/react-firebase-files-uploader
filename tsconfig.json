{
    "compilerOptions": {
        "target": "ESNext",
        "module": "commonjs",
        "jsx": "react",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "strict": true,
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true
    },
    "include": ["src"],
    "exclude": ["node_modules"]
}
